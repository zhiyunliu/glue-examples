{
	"app":{
		"mode":"release",
		"ip_mask":"192.168"
	},
	"registry":"nacos://aliyun",	
 	"queues":{
		"redis":{"proto":"redis","addr":"redis://localhost"},
		"streamredis":{"proto":"streamredis","addr":"redis://localhost"}
	},
	"nacos":{
		"aliyun":{
			"encrypt":false,
			"client":{"namespace_id":""},
			"server":[{"ipaddr":"localhost","port":8848}],
			"options":{"prefix":"api","group":"charge","cluster":"grey","weight":100}
		}
	},
	"redis":{
		"localhost":{
			"addrs":["192.168.0.120:6379"],
			"db":0,
			"dial_timeout":10,
			"read_timeout":10,
			"write_timeout":10,
			"pool_size":20
		}
	},
    "servers":{
		"apiserver":{
			"config":{"addr":":8084","status":"start","read_timeout":10,"write_timeout":10,"read_header_timeout":10,"max_header_bytes":65535},
 			"header":{"a":"1","b":"2"}
		},
		"mqcserver_redis":{
			"config":{"addr":"queues://redis","status":"start"},
			"tasks":[{"queue":"redis"}]
		},
		"mqcserver_streamredis":{
			"config":{"addr":"queues://streamredis","status":"start"},
			"tasks":[{"queue":"streamredis"}]
		}
    }
}