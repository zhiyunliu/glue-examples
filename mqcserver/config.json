{
	"app":{
		"mode":"release",
		"ip_mask":"192.168"
	},
	"registry":"nacos://aliyun",	
 	"queues":{
		"redis":{"proto":"redis","addr":"redis://localhost","enable_deadletter":true},
		"streamredis":{"proto":"streamredis","addr":"redis://localhost","enable_deadletter":true}
	},
	"nacos":{
		"aliyun":{
			"encrypt":false,
			"client":{"namespace_id":""},
			"server":[{"ipaddr":"localhost","port":8848}],
			"options":{"prefix":"api","group":"charge","cluster":"grey","weight":100}
		}
	},
	"redis":{
		"localhost":{
			"addrs":["192.168.3.27:6379"],
			"password":"",
			"db":14,
			"dial_timeout":10,
			"read_timeout":10,
			"write_timeout":10,
			"pool_size":100
		}
	},
    "servers":{
		"apiserver":{
			"config":{"addr":":8084","status":"start","read_timeout":10,"write_timeout":10,"read_header_timeout":10,"max_header_bytes":65535},
 			"header":{"a":"1","b":"2"}
		},
		"mqcserver_redis":{
			"config":{"addr":"queues://redis","status":"start"},
			"tasks":[
				{"queue":"redis","concurrency":10},
				{"queue":"redis2","concurrency":10}
			]
		},
		"mqcserver_streamredis":{
			"config":{"addr":"queues://streamredis","status":"start"},
			"tasks":[
				{"queue":"streamredis","concurrency":10},
				{"queue":"streamredis2","concurrency":10}
			]
		}
    }
}