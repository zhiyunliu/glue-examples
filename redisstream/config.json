{
	"app":{
		"mode":"release",
		"ip_mask":"192.168"
	},
	"queues":{				 
 		"streamredis":{"proto":"streamredis","addr":"redis://0181","stream_max_len":100,"concurrency":100,"buffer_size":100,"blocking_timeout":1}
	},
	"redis":{
		"0181":{
			"addrs":["192.168.0.181:6379"],
			"db":1,
			"dial_timeout":10,
			"read_timeout":10,
			"write_timeout":10,
			"pool_size":20
		}
	},
    "servers":{
		"apiserver":{
			"config":{"addr":":8084","status":"start","read_timeout":10,"write_timeout":10,"read_header_timeout":10,"max_header_bytes":65535},
			"middlewares":[{"name":"jwt","data":{"secret":"123456789","method":"sha256","expire":"3600","excludes":["/log/**","/demo/**","/dlock/**","/queue/**"]}}],			
			"header":{"a":"1","b":"2"}
		},
		"mqcserver":{
			"config":{"addr":"queues://streamredis","status":"start"},
			"tasks":[{"queue":"queue1"}]
		}
    }
}