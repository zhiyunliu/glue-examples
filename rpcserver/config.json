{
	"app":{
		"mode":"release",
		"ip_mask":"192.168",
        "graceful_shutdown_timeout":15,
		"dependencies":["After=network.target"],
		"options":{"LimitNOFILE":10240,"MaxOPENFiles":4096}
	},
	"registry":"nacos://aliyun",	
	"config":"nacos://aliyun",
	"queues":{
		"default":"redis://localhost"		
	},
	"rpcs":{
		"default":{}	
	},
	"nacos":{
		"aliyun":{
			"encrypt":false,
			"client":{"namespace_id":"365a3498-120a-4b56-8322-1990954b1675"},
			"server":[{"ipaddr":"localhost","port":8848}],
			"options":{"prefix":"api","group":"charge","cluster":"grey","weight":100}
		}
	},
	"redis":{
		"localhost":{
			"addrs":["localhost:6379"],
			"db":0,
			"dial_timeout":10,
			"read_timeout":10,
			"write_timeout":10,
			"pool_size":20
		}
	},
    "servers":{
		"rpc":{
			"config":{"addr":":7080","status":"start"},
			"middlewares":[]
		}
    }
}