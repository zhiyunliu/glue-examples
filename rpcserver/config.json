{
	"app":{
		"mode":"release",
		"ip_mask":"192.168"
	},
	"registry":"nacos://aliyun",	
 	"queues":{
		"default":"redis://localhost"		
	},
	"rpcs":{
		"default":{}	
	},
	"nacos":{
		"aliyun":{
			"encrypt":false,
			"client":{"namespace_id":""},
			"server":[{"ipaddr":"localhost","port":8848}],
			"options":{"prefix":"api","group":"charge","cluster":"grey","weight":100}
		}
	},
	"redis":{
		"localhost":{
			"addrs":["localhost:6379"],
			"db":0,
			"dial_timeout":10,
			"read_timeout":10,
			"write_timeout":10,
			"pool_size":20
		}
	},
    "servers":{
		"rpcserver":{
			"config":{"addr":":7080","status":"start"},
			"middlewares":[]
		}
    }
}