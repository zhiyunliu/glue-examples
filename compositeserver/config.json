{
	"app":{
		"mode":"release",
		"ip_mask":"192.168",
		"trace_addr":":18182",
		"dependencies":["After=network.target"],
		"options":{"LimitNOFILE":10240,"MaxOPENFiles":4096}
	},
	"registry":"nacos://default",	 
	"dbs":{
		"localhost":{"proto":"mysql","conn":"root:123456@tcp(localhost)/demo?charset=utf8","max_open":10,"max_idle":10,"life_time":100},
		"microsql":{"proto":"sqlserver","conn":"server=localhost;database=demo;persist security info=false;user id=admin;password=123456;Min Pool Size=10;Max Pool Size=20","max_open":10,"max_idle":10,"life_time":100}
	},
	"rpcs":{
		"default":{"proto":"grpc","trace":true}
	},
	"xhttp":{
		"default":{"balancer":"wrr","conn_timeout":10,"trace":true}
	},
	"caches":{
		"default":{"proto":"redis","addr":"redis://localhost"}
	},
	"queues":{				 
		"default":{"proto":"redis","addr":"redis://localhost"},
		"streamredis":{"proto":"redis","addr":"redis://localhost"}
	},
	"redis":{
		"localhost":{
			"addrs":["192.168.3.27:6379"],
			"password":"Xunyou2022",
			"db":0,
			"dial_timeout":10,
			"read_timeout":10,
			"write_timeout":10,
			"pool_size":20
		}
	},
	"nacos":{
		"default":{
			"encrypt":false,
			"client":{"namespace_id":""},
			"server":[{"ipaddr":"192.168.3.27","port":8848}],
			"options":{"prefix":"api","group":"charge","cluster":"grey","weight":100}
		}
	},
	"prometheus":{
		"counter":{
			"namespace":"server",
			"subsystem":"requests",
			"name":"code_total",
			"help":"The total number of processed requests",
			"labels":["kind", "path", "code", "reason"]
		},
		"histogram":{
			"namespace":"server",
			"subsystem":"requests",
			"name":"duration_sec",
			"help":"server requests duration(sec)",
			"buckets":[0.05, 0.1, 0.5, 1, 1.5, 2, 2.5, 3, 4, 5],
			"labels":["kind", "path"]
		},
		"gauge":{
			"namespace":"server",
			"subsystem":"requests",
			"name":"cur_proc",
			"help":"server current processing.",
			"labels":["kind", "path"]
		}
	},
    "servers":{
		"apiserver":{
			"config":{"addr":":8080","status":"start"}
		},
		"mqcserver":{
			"config":{"addr":"queues://streamredis","status":"start"},
			"tasks":[{"queue":"yy.xx.xx","service":"/demomqc"}]
		},
		"rpcserver":{
			"config":{"addr":":8081","status":"start"}
		},
		"cronserver":{
			"config":{"status":"start","sharding":1},
			"jobs":[
				{"cron":"@every 1s","immediately":true,"service":"/democron","meta":{"a":1}} 
			]
	    }
	}
}